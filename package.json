{
  "name": "@financial-times/n-magnet",
  "version": "0.0.0",
  "description": "orchestrates onward journey components (better promo, newsletters, etc)",
  "main": "main.js",
  "scripts": {
    "jest": "jest",
    "webpack": "webpack",
    "precommit": "node_modules/.bin/secret-squirrel",
    "commitmsg": "node_modules/.bin/secret-squirrel-commitmsg",
    "prepush": "make verify -j3",
    "build-dev": "webpack --config webpack.config.js --mode development --env development",
    "build-prod": "webpack --config webpack.config.js --mode production --env production",
    "prepare": "npx snyk protect || npx snyk protect -d || true",
    "preinstall": "[ \"$INIT_CWD\" != \"$PWD\" ] || npm_config_yes=true npx check-engine",
    "build": "dotcom-tool-kit build:local",
    "start": "dotcom-tool-kit run:local",
    "test": "dotcom-tool-kit test:local",
    "heroku-postbuild": "dotcom-tool-kit build:remote release:remote cleanup:remote",
    "demo": "npm run demo-build && node demos/app",
    "demo-build": "rm -rf ./dist && npm run webpack-styles-dev && npm run webpack-js-dev",
    "webpack-styles-dev": "webpack --config webpack.styles.config.js --env development --mode development",
    "webpack-js-dev": "webpack --config webpack.js.config.js --env development --mode development",
    "check-secret": "secret-squirrel init"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Financial-Times/n-magnet.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Financial-Times/n-magnet/issues"
  },
  "homepage": "https://github.com/Financial-Times/n-magnet#readme",
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.18.9",
    "@dotcom-tool-kit/backend-app": "^2.0.5",
    "@dotcom-tool-kit/jest": "^2.0.3",
    "@financial-times/n-express": "^23.0.3",
    "@financial-times/secret-squirrel": "^2.21.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "bower": "^1.8.8",
    "check-engine": "^1.10.1",
    "css-loader": "^1.0.0",
    "dotcom-tool-kit": "^2.2.3",
    "eslint": "^5.12.1",
    "eslint-plugin-react": "^7.12.4",
    "fetch-mock": "^6.4.3",
    "file-loader": "^2.0.0",
    "husky": "^8.0.1",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.4.4",
    "pa11y-ci": "^2.1.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sass": "^1.50.1",
    "sass-loader": "^7.1.0",
    "snyk": "^1.167.2",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@financial-times/dotcom-server-handlebars": "^7.0.0",
    "@financial-times/ft-date-format": "^1.0.0",
    "@financial-times/n-eventpromo": "^10.0.0",
    "@financial-times/n-newsletter-signup": "^9.1.0",
    "@financial-times/x-engine": "^1.0.2",
    "handlebars-loader": "^1.7.0",
    "js-cookie": "^2.2.0",
    "mathsass": "0.10.1"
  },
  "peerDependencies": {
    "@financial-times/o-labels": "^6.2.2",
    "n-ui-foundations": "^9.0.0"
  },
  "x-dash": {
    "engine": {
      "server": "react",
      "browser": "react"
    }
  },
  "engines": {
    "node": "14.x || 16.x",
    "npm": "7.x || 8.x"
  },
  "volta": {
    "node": "16.14.2"
  }
}
